<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" style="@style/XList.Singe">
    <RelativeLayout android:id="@+id/x_list_left_lay" android:layout_width="@dimen/x_list_left_width" android:layout_height="match_parent">
        <com.xiaopeng.xui.widget.XTextView android:id="@+id/x_list_num" android:visibility="gone" app:vuiMode="DISABLED" style="@style/XListTextNum"/>
        <ViewStub android:id="@+id/x_list_left" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </RelativeLayout>
    <com.xiaopeng.xui.widget.XTextView android:id="@+id/x_list_tv" android:layout_marginStart="@dimen/x_list_left_width" android:layout_marginEnd="@dimen/x_list_text_padding_end" app:vuiMode="DISABLED" style="@style/XListText"/>
    <com.xiaopeng.xui.widget.XImageView android:id="@+id/x_list_tag" android:layout_alignTop="@+id/x_list_tv" android:layout_alignBottom="@+id/x_list_tv" android:layout_marginStart="@dimen/x_list_icon_margin_left_width" android:layout_toEndOf="@+id/x_list_tv" app:vuiMode="DISABLED" style="@style/XListIconTag"/>
    <ViewStub android:id="@+id/x_list_right" android:layout_width="@dimen/x_list_right_width" android:layout_height="match_parent" android:layout_centerVertical="true" android:layout_alignParentEnd="true"/>
</merge>
