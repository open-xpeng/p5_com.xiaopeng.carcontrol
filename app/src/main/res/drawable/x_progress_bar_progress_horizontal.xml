<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="start|center_vertical" android:id="@android:id/background" android:height="6dp">
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <solid android:color="@color/x_theme_primary_neutral_normal"/>
        </shape>
    </item>
    <item android:gravity="start|center_vertical" android:id="@android:id/secondaryProgress" android:height="6dp">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <corners android:radius="3dp"/>
                <solid android:color="@color/x_theme_primary_disable"/>
            </shape>
        </scale>
    </item>
    <item android:gravity="start|center_vertical" android:id="@android:id/progress" android:height="6dp">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <corners android:radius="3dp"/>
                <solid android:color="@color/x_theme_primary_normal"/>
            </shape>
        </scale>
    </item>
</layer-list>
