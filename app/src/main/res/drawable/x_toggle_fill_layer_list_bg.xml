<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/x_toggle_rect_round_radius"/>
            <solid android:color="@color/x_primary_neutral_color_selector"/>
        </shape>
    </item>
    <item android:id="@android:id/toggle">
        <drawable app:toggle_style="fill" class="com.xiaopeng.xui.drawable.XToggleDrawable"/>
    </item>
    <item>
        <drawable app:rippleRadius="@dimen/x_toggle_rect_round_radius" app:rippleSupportScale="true" class="com.xiaopeng.xui.drawable.ripple.XRippleDrawable"/>
    </item>
</layer-list>
